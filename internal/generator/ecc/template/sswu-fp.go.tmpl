{{ define "eval_poly" }}
{{/*arguments: destination, monic, coefficients, variable*/}}
*{{$.V1}} = {{last $.V3}}

if {{$.V2}} {
   {{$.V1}}.Add({{$.V1}}, &{{$.V4}})
}

{{- range $c := reverse $.V3}}
{{$.V1}}.Mul({{$.V1}}, {{$.V4}})
{{$.V1}}.Mul({{$.V1}}, fp.Element{ {{$c}} })
{{- end}}
{{end}}

{{ if notNil $.HashInfo.E1Prime }}
func isogeny_x_numerator(dst *fp.Element, src *fp.Element) {
    {{ template "eval_poly" "dst" "False" $.HashInfo.E1Prime.XMap.Num "src" }}
}
{{ end }}